{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": ["./src"],
                "extensions": [".js", ".ios.js", ".android.js"],
                "alias": {
                    "_App":"./app",
                    "_Fonts": "./app/assets/fonts",
                    "_Images": "./app/assets/images",
                    "_Themes": "./app/assets/themes",
                    "_Components": "./app/components",
                    "_StyledComps": "./app/components/styled-components",
                    "_Context": "./app/context",
                    "_I18Next": "./app/i18n",
                    "_Screens": "./app/screens",
                    "_Utils": "./app/utils"
                }
            }
        ]
    ]
}